import sys

def calculator(formula):
    try:
        a, sign, b = formula.split()
        a = int(a)  
        b = int(b)

        if sign == '+':
            result = a + b
        elif sign == '-':
            result = a - b
        elif sign == '*':
            result = a * b
        elif sign == '/':
            if b == 0:
                raise ValueError("Error: ゼロ除算")
            result = a / b
        else:
            raise ValueError("Error: 無効な操作")

        print(result)

    except ValueError as e:
        print(f"Error: {e}")
        sys.exit(1)
    except Exception:
        print("Error: 無効な入力操作")
        sys.exit(1)

if __name__ == "__main__":
    for line in sys.stdin:
        calculator(line.strip())

